[
    {
        "key": "alt+m",
        "command": "workbench.action.toggleMaximizedPanel"
    },
    {
        "key": "ctrl+shift+d",
        "command": "supermaven.editWithSupermaven"
    },
    {
        "key": "ctrl+i",
        "command": "-supermaven.editWithSupermaven"
    },
    {
        "key": "ctrl+shift+e",
        "command": "supermaven.addToChat"
    },
    {
        "key": "ctrl+shift+i",
        "command": "-supermaven.addToChat"
    },
    {
        "key": "ctrl+shift+i",
        "command": "continue.quickEdit"
    },
    {
        "key": "ctrl+i",
        "command": "-continue.quickEdit"
    },
    {
        "key": "ctrl+shift+o",
        "command": "continue.optimizeCode"
    },
    {
        "key": "ctrl+shift+f",
        "command": "continue.fixCode"
    }
]
