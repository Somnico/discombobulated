                    }
                },
                {
                    "scope": "constant.numeric.decimal.js",
                    "settings": {
                        // "foreground": "#FFEB95"
                        "foreground": "#d1ca8d"
                    }
                },
                {
                    "scope": "storage.type.js",
                    "settings": {
                        "foreground": "#569CD6"
                    }
                },
                {
                    "scope": "keyword.operator.assignment.js",
                    "settings": {
                        "foreground": "#ffffff"
                    }
                },
                {
                    "scope": "punctuation.accessor.js",
                    "settings": {
                        "foreground": "#ffffff"
                    }
                },
                {
                    "scope": "entity.name.function.js",
                    "settings": {
                        "foreground": "#80CBC4"
                    }
                },
                {
                    "scope": "string.quoted.single.js",
                    "settings": {
                        // "foreground": "#C3E88D",
                        "foreground": "#aee88d"
                    }
                }
            ]
        },
        "[Bearded Themanopia (Experimental)]": {
            "textMateRules": [
                {
                    "scope": "entity.other.attribute-name.js.jsx",
                    "settings": {
                        "foreground": "#73c283"
                    }
                }
            ]
        },
        "[Tokyo Night Storm]": {
            "textMateRules": [
                {
                    "scope": "editor.findMatchHighlightBorder",
                    "settings": {
                        "foreground": "#ffffff00"
                    }
                },
                {
                    "scope": "entity.other.attribute-name.js",
                    "settings": {
                        // "foreground": "#c2b073"
                    }
                },
                {
                    "scope": "entity.name.tag.tsx, support.class.component.tsx, support.class.component.js",
                    "settings": {
                        "foreground": "#F7768E"
                    }
                },
                {
                    "scope": [
                        "string.quoted.double.json.comments",
                        "string.quoted.double.html",
                        "string.quoted.double.js",
                        "string.quoted.double.tsx",
                        "string.quoted.double.css",
                        "string.quoted.single",
                        "entity.other.attribute-name.class.css",
                        "support.constant.font-name.css"
                    ],
                    "settings": {
                        // "foreground": "#F5DEC1"
                        "foreground": "#7dccff"
                    }
                },
                {
                    "scope": [
                        "storage.type.js",
                        "storage.type.tsx",
                        "keyword.control.flow.js",
                        "keyword.control.flow.tsx",
                        "storage.modifier.typeset.shell",
                        "storage.modifier.local.shell"
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                }
            ]
        }
    },
