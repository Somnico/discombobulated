{
    "editor.tokenColorCustomizations": {
        "[Palenight Theme]": {
            "textMateRules": [
                {
                    "scope": "entity.name.tag.html",
                    "settings": {
                        "foreground": "#8873c2"
                    }
                },
                {
                    "scope": "variable.other.readwrite.js",
                    "settings": {
                        // "foreground": "#80CBC4"
                        "foreground": "#9CDCFE"
                    }
                },
                {
                    "scope": "variable.other.object.js",
